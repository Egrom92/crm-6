Users/roman/PhpstormProjects/training/crm-4/src/pages/NewOrderPage.jsx",[],"/Users/roman/PhpstormProjects/training/crm-4/src/pages/OrderPage.jsx",[],"/Users/roman/PhpstormProjects/training/crm-4/src/pages/ManagerPage/index.jsx",["260"],"import React, { useState, useEffect } from \"react\";\nimport OrderFilter from \"./OrderFilter\";\nimport Orders from \"./Orders\";\nimport { NavLink } from \"react-router-dom\";\nimport hc from \"../../hc\";\nimport Pagination from \"./Pagination\";\n\nexport default function Table(props) {\n  const [orders, setOrders] = useState([]);\n  const [products, setProducts] = useState([]);\n  const [queryParams, setQueryParams] = useState([])\n\n  useEffect(() => {\n    hc.get(\"/products\", { _limit: 1000 }).then((products) =>\n      setProducts(products)\n    );\n  }, []);\n\n  useEffect(() => {\n    hc.get(\"/orders\", queryParams).then((orders) => setOrders(orders));\n  }, []);\n\n  return (\n    <main\n      role=\"main\"\n      className=\"col-md-9 ml-sm-auto col-lg-10 px-4 d-flex flex-column\"\n    >\n      <div className=\"d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom\">\n        <h1 className=\"h2\">Заказы</h1>\n        <NavLink to=\"/new\" className=\"btn btn-outline-success\">\n          Добавить заказ\n        </NavLink>\n      </div>\n\n      <OrderFilter queryParam={queryParams} setQueryParams={setQueryParams} setOrders={setOrders} orders={orders} />\n\n      <Orders orders={orders} products={products} />\n\n      <Pagination orders={orders} setOrders={setOrders} />\n    </main>\n  );\n}\n","/Users/roman/PhpstormProjects/training/crm-4/src/hc.js",[],"/Users/roman/PhpstormProjects/training/crm-4/src/data.js",[],"/Users/roman/PhpstormProjects/training/crm-4/src/pages/ManagerPage/Orders.jsx",["261","262"],"import React, { useEffect, useState } from \"react\";\nimport { statuses } from \"../../data\";\nimport { NavLink } from \"react-router-dom\";\n\n\nimport Table from \"../../components/Table\";\n\nconst statusClass = {\n  new: \"badge-primary\",\n  process: \"badge-warning\",\n  back: \"badge-danger\",\n  archived: \"badge-dark\"\n};\n\nexport default function Orders(props) {\n  const { orders, products } = props;\n\n  const fields = [\n    { label: \"ID\", name: \"id\" },\n    { label: \"ИФО\", name: \"fullname\" },\n    {\n      label: \"Заказ\",\n      render(order) {\n        return products.find((x) => x.id === order.productId)?.name;\n      }\n    },\n    {\n      label: \"Статус\",\n      render({ status }) {\n        const statusData = statuses.find((x) => x.value === status);\n\n        return (\n          <span className={`badge ${statusClass[statusData.value]}`}>\n            {statusData.name}\n          </span>\n        );\n      }\n    },\n    {\n      label: \"Цена\",\n      render(order) {\n        return products.find((x) => x.id === order.productId)?.price;\n      }\n    },\n    {\n      label: \"Действия\",\n      render(order) {\n        return (\n          <NavLink\n            to={`/order/${order.id}`}\n            className=\"btn btn-outline-primary btn-sm\"\n          >\n            Редактировать\n          </NavLink>\n        );\n      }\n    },\n    {\n      label: \"Дата\",\n      render(order) {\n        const date = new Date(order.createdAt);\n        const dateString = `${date.getFullYear()}-${(date.getMonth() + 1)\n          .toString()\n          .padStart(2, \"0\")}-${date.getDate().toString().padStart(2, \"0\")}`;\n\n        return dateString;\n      }\n    }\n  ];\n\n  return (\n    <Table className=\"table table-striped\" fields={fields} items={orders} />\n  );\n}\n","/Users/roman/PhpstormProjects/training/crm-4/src/pages/ManagerPage/OrderFilter.jsx",["263","264","265"],"import React, {useState, useEffect} from 'react'\nimport hc from \"../../hc\";\nimport { statuses } from \"../../data\";\nimport {useHistory} from 'react-router-dom';\n\nexport default function OrderFilter(props) {\n    const history = useHistory();\n\n    const {setOrders, setQueryParams, queryParams} = props\n\n    const [products, setProducts] = useState([]);\n    const [productId, setProductId] = useState('all');\n    const [status, setStatus] = useState(\"all\");\n    const [fullName, setFullName] = useState('');\n\n\n    const userSort = async (title, direction) => {\n        const userList = await hc.get(\"/users\", {\n            _sort: title,\n            _order: direction\n        });\n\n        setOrders(userList);\n    };\n\n    // const getQueryParams = () => {\n    //     let queryes = history.location.search.slice(1).split('&')\n    //\n    //     queryes = queryes.map(el => el.split('='))\n    //     let result = queryes.reduce((acc, item) => {\n    //         acc[item[0]] = item[1]\n    //\n    //         return acc\n    //     }, {})\n    //     setQueryParams(result)\n    // }\n\n    useEffect(() => {\n        hc.get(\"/products\", { _limit: 1000 }).then((products) =>\n            setProducts(products)\n        );\n    }, []);\n\n    const filterStatus = (e) => {\n        const status = e.target.value;\n        setStatus(status);\n\n\n        history.push({\n            pathname: '/',\n            search: \"?\" + new URLSearchParams({status: status}).toString()\n        });\n\n        hc.get(\"/orders\", {status: status}).then((orders) => setOrders(orders))\n    };\n\n    const filterOrder = (e) => {\n        const productId = e.target.value\n        setProductId(productId);\n\n        history.push({\n            pathname: '/',\n            search: \"?\" + new URLSearchParams({productId: productId}).toString()\n        })\n\n        hc.get(\"/orders\", {productId: productId}).then((orders) => setOrders(orders))\n    };\n\n    const filterFullname = (e) => {\n        const newFullName = e.target.value\n        console.log(newFullName)\n        \n        setFullName(newFullName)\n\n        hc.get(\"/orders\", {fullname: fullName}).then((orders) => setOrders(orders))\n    }\n\n    return (\n        <div className=\"form-row p-2\">\n            <div className=\"form-group col\">\n                <label>ФИО:</label>\n                <input onInput={(e) => filterFullname(e)} className=\"form-control\" type=\"text\" placeholder=\"ФИО\"/>\n            </div>\n            <div className=\"form-group col\">\n                <label>Заказ:</label>\n                <select\n                    className=\"form-control\"\n                    value={productId}\n                    onChange={(e) => filterOrder(e)}\n                >\n                    <option key={0} value={\"all\"}>\n                        Все\n                    </option>\n                    {products.map((product, i) => (\n                        <option key={i + 1} value={product.id}>\n                            {product.name}\n                        </option>\n                    ))}\n                </select>\n\n            </div>\n            <div className=\"form-group col\">\n                <label>Статус:</label>\n                <select\n                    className=\"form-control\"\n                    value={status}\n                    onChange={(e) => filterStatus(e)}\n                >\n                    <option key={0} value={\"all\"}>\n                        Все\n                    </option>\n                    {statuses.map((status, i) => (\n                        <option key={i + 1} value={status.value}>\n                            {status.name}\n                        </option>\n                    ))}\n                </select>\n\n\n            </div>\n            <div className=\"form-group col\">\n                <label>Сумма:</label>\n                <input className=\"form-control mb-2\" type=\"number\" placeholder=\"От\"/>\n                <input className=\"form-control\" type=\"number\" placeholder=\"До\"/>\n            </div>\n            <div className=\"form-group col\">\n                <label>Дата:</label>\n                <input className=\"form-control mb-2\" type=\"date\"/>\n                <input className=\"form-control\" type=\"date\"/>\n            </div>\n        </div>\n    )\n}","/Users/roman/PhpstormProjects/training/crm-4/src/pages/ManagerPage/Pagination.jsx",[],"/Users/roman/PhpstormProjects/training/crm-4/src/HttpClient.jsx",[],"/Users/roman/PhpstormProjects/training/crm-4/src/components/Table.jsx",[],["266","267"],"/Users/roman/PhpstormProjects/training/crm-5/src/index.js",[],["268","269"],"/Users/roman/PhpstormProjects/training/crm-5/src/reportWebVitals.js",[],"/Users/roman/PhpstormProjects/training/crm-5/src/App.js",[],"/Users/roman/PhpstormProjects/training/crm-5/src/pages/NewOrderPage.jsx",[],"/Users/roman/PhpstormProjects/training/crm-5/src/pages/OrderPage.jsx",[],"/Users/roman/PhpstormProjects/training/crm-5/src/pages/ManagerPage/index.jsx",[],"/Users/roman/PhpstormProjects/training/crm-5/src/hc.js",[],"/Users/roman/PhpstormProjects/training/crm-5/src/data.js",[],"/Users/roman/PhpstormProjects/training/crm-5/src/hooks/useOnce.js",["270","271"],"import { useEffect } from \"react\";\n\nexport default (callback) => {\n  useEffect(() => {\n    callback();\n  }, []);\n};\n","/Users/roman/PhpstormProjects/training/crm-5/src/hooks/useQuery.js",["272"],"import { useEffect, useState } from \"react\";\nimport { useLocation } from \"react-router-dom\";\n\nexport default () => {\n  const location = useLocation();\n  const [query, setQuery] = useState({});\n\n  useEffect(() => {\n    setQuery(\n      Object.fromEntries(\n        location.search\n          .slice(1)\n          .split(\"&\")\n          .map((pair) => pair.split(\"=\"))\n      )\n    );\n  }, [location.search]);\n\n  return query;\n};\n","/Users/roman/PhpstormProjects/training/crm-5/src/pages/ManagerPage/OrderFilter.jsx",[],"/Users/roman/PhpstormProjects/training/crm-5/src/pages/ManagerPage/Orders.jsx",["273","274"],"/Users/roman/PhpstormProjects/training/crm-5/src/pages/ManagerPage/Pagination.jsx",[],"/Users/roman/Phpsto